-GROUP TinySnbUpdateTest
-DATASET tinysnb

--

-CASE SetNodeInt64PropTest
-STATEMENT MATCH (a:person) WHERE a.ID=0 SET a.age=20 + 50
---- ok
-QUERY MATCH (a:person) WHERE a.ID=0 RETURN a.age
---- 1
70

